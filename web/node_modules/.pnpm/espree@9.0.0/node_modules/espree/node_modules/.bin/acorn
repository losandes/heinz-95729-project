#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/node_modules:/mnt/c/Users/Ishita/source/repos/node_modules:/mnt/c/Users/Ishita/source/node_modules:/mnt/c/Users/Ishita/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/acorn/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/espree/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/acorn@8.5.0/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/node_modules:/mnt/c/Users/Ishita/source/repos/node_modules:/mnt/c/Users/Ishita/source/node_modules:/mnt/c/Users/Ishita/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/acorn/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules:/mnt/c/Users/Ishita/source/repos/heinz-95729-project/web/node_modules/.pnpm/espree@9.0.0/node_modules/espree/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../acorn/bin/acorn" "$@"
fi
