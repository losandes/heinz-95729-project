{
  "name": "heinz-95729-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "// INSTALL": "==========================INSTALL============================",
    "init:env": "node ./bin/init-env.js",
    "// DEV": "================================DEV==============================",
    "dev": "vite",
    "// BUILD": "=============================BUILD=============================",
    "build": "tsc && vite build",
    "// PREVIEW": "==========================PREVIEW============================",
    "preview": "vite preview",
    "// LINT": "==============================LINT==============================",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "// TEST": "==============================TEST==============================",
    "_test": "cross-env-shell vitest run",
    "_test:ci": "cross-env-shell vitest run --no-color --coverage",
    "_test:coverage": "cross-env-shell vitest run --coverage",
    "_test:watch": "cross-env-shell vitest",
    "test": "cross-env NODE_ENV='cicd' PUBLIC_LOG_LEVELS='off' pnpm _test",
    "test:ci": "cross-env NODE_ENV='cicd' PUBLIC_LOG_LEVELS='off' pnpm _test:ci",
    "test:coverage": "cross-env NODE_ENV='cicd' PUBLIC_LOG_LEVELS='off' pnpm _test:coverage",
    "test:watch": "cross-env NODE_ENV='cicd' PUBLIC_LOG_LEVELS='off' pnpm _test:watch"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.6.2",
    "pnpm": "^8.10.5",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
